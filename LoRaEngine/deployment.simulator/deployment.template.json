{
  "moduleContent": {
    "$edgeAgent": {
      "properties.desired": {
        "schemaVersion": "1.0",
        "runtime": {
          "type": "docker",
          "settings": {
            "minDockerVersion": "v1.25",
            "loggingOptions": "",
            "registryCredentials": {
              "$CONTAINER_REGISTRY_USERNAME": {
                "username": "$CONTAINER_REGISTRY_USERNAME",
                "password": "$CONTAINER_REGISTRY_PASSWORD",
                "address": "$CONTAINER_REGISTRY_ADDRESS"
              }
            }
          }
        },
        "systemModules": {
          "edgeAgent": {
            "type": "docker",
            "settings": {
              "image": "mcr.microsoft.com/azureiotedge-agent:1.0.3",
              "createOptions": ""
            }
          },
          "edgeHub": {
            "type": "docker",
            "status": "running",
            "restartPolicy": "always",
            "settings": {
              "image": "mcr.microsoft.com/azureiotedge-hub:1.0.3",
              "createOptions": "{\"HostConfig\":{\"PortBindings\":{\"8883/tcp\":[{\"HostPort\":\"8883\"}],\"443/tcp\":[{\"HostPort\":\"443\"}]}}}"
            },
            "env": {
              "OptimizeForPerformance": {
                "value": "false"
              }
            }
          }
        },
        "modules": {
          "LoRaWanNetworkSrvModule": {
            "type": "docker",
            "settings": {
              "image": "${MODULES.LoRaWanNetworkSrvModule.arm32v7}",
              "createOptions": "{  \"ExposedPorts\": {    \"1680/udp\": {}  },  \"HostConfig\": {    \"PortBindings\": {      \"1680/udp\": [        {          \"HostPort\": \"1680\",          \"HostIp\":\"172.17.0.1\"        }      ]    }  }}"
            },
            "version": "1.0",
            "env": {
              "LOG_LEVEL": {
                "value": "0"
              },
              "LOG_TO_HUB": {
                "value": "true"
              },
              "IOTEDGE_TIMEOUT": {
                "value": "$IOTEDGE_TIMEOUT"
              }
            },
            "status": "running",
            "restartPolicy": "always"
          },
          "LoRaSimulator": {
            "type": "docker",
            "settings": {
              "image": "${MODULES.LoRaSimulator.arm32v7}",
              "createOptions": "{  \"ExposedPorts\": {    \"$SIMULATOR_PORT/udp\": {}  },  \"HostConfig\": {    \"PortBindings\": {      \"$SIMULATOR_PORT/udp\": [        {          \"HostPort\": \"$SIMULATOR_PORT\",          \"HostIp\":\"172.17.0.1\"        }      ]    }  }}"
            },              
            "env": {
              "RESET_PIN": {
                "value": "$RESET_PIN"
              },
              "REGION": {
                "value": "$REGION"
              },
              "SIMULATOR_PORT": {
                "value": "$SIMULATOR_PORT"
              }
            },
            "version": "1.0",
            "status": "running",
            "restartPolicy": "always"
          }
        }
      }
    },
    "$edgeHub": {
      "properties.desired": {
        "schemaVersion": "1.0",
        "routes": {
          "route": "FROM /* INTO $upstream"
        },
        "storeAndForwardConfiguration": {
          "timeToLiveSecs": 7200
        }
      }
    },
    "LoRaWanNetworkSrvModule": {
      "properties.desired": {
        "schemaVersion": "1.0",
        "FacadeServerUrl": "$FACADE_SERVER_URL",
        "FacadeAuthCode": "$FACADE_AUTH_CODE"
      }
    }
  }
}
